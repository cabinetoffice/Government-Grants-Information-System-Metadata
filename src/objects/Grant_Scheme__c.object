    <validationRules>
        <fullName>All_Fields_Are_Locked_Apart_From_Eval</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 NOT($Setup.Bypass__c.Validation_Rules__c)
 , NOT( $Profile.Name = &apos;System Administrator&apos;) ,NOT($Setup.Bypass__c.Business_Rules_for_Edit_and_Delete__c)
 , RecordType.DeveloperName = &quot;Approved_Grant_Scheme&quot;
 , ISPICKVAL($Profile.UserType, &apos;PowerCustomerSuccess&apos;)
 , NOT(ISNEW())
 , OR(
  AND(
   ISBLANK(Max_Grant_Award_End_Date__c)
   , NOT(ISBLANK(TEXT(Duration__c)))
   , NOT(ISPICKVAL(Duration__c, &apos;Unknown&apos;))
   , ISBLANK(Max_Grant_Award_End_Date__c)
   , (TODAY() - End_Date__c ) &gt; 365
  )
  , (TODAY() - Max_Grant_Award_End_Date__c) &gt; 365
 )
 , NOT(ISCHANGED(Scheme_Evalution_Completed__c ))
 , NOT(ISCHANGED(If_No_Reasons__c))
 , NOT(ISCHANGED(Scheme_Objective__c))
 , NOT(ISCHANGED(Reason_for_Given_Answer__c))
)</errorConditionFormula>
        <errorMessage>This record is locked. Please create a case or contact the grants hub support in order to modify this record.</errorMessage>
